<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usingparselogin"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />
<uses-permission android:name="android.permission.INTERNET" />
 <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:name="ParseApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.usingparselogin.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.usingparselogin.LoginActivity" >
        </activity>
        <activity android:name="com.example.usingparselogin.SignUp" >
        </activity>
        <activity android:name="com.example.usingparselogin.Welcome" >
        </activity>
        <activity android:name="com.example.usingparselogin.ForgetParsePassword">    
        </activity>
        <activity android:name="com.example.pedometer_sample_project.Pedometer">
        </activity>
        <activity android:name="com.example.pedometer_sample_project.Settings"
                android:label="@string/activity_settings">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <service android:name="com.example.pedometer_sample_project.StepService"/>
    </application>
</manifest>
